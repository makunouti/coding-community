      <thead>
        <th>会員ID</th>
        <th>氏名</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
      </tead>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= link_to (user.name),admin_user_path(user.id) %></td>
          <td><%= user.email %></td>
          <td>
            <% if user.is_active == true %>
              <b class="text-muted">退会</b>
            <% else %>
              <b class="text-success">有効</b>
            <% end %>
          </td>
        </tr>
      <% end %>
  </table>
</div>
<div class="d-flex justify-content-center">
  <%= paginate @users %>
</div>